{
    "$schema": "./node_modules/wrangler/config-schema.json",
    "name": "ytyango", // 可改为你项目名称
    "compatibility_date": "2025-12-10", // 建议设为今天或你想要的兼容日期
    "assets": {
        "directory": "./build", // 指向你的 build 输出目录
        // 可选项，适用于 SPA fallback 或自定义 404 页面：
        // "not_found_handling": "single-page-application"
    },
    // 如果你还希望启用 Wrangler 的 preview / workers_dev，可加上：
    // "workers_dev": true,
    // "preview": true,
    // 如果你有 environment variables / KV 等绑定，也可以在这里配置
}